{
    "name": "lmphilippines",
    "version": "1.0.0",
    "description":
        "Modern Jekyll based blog. Great for companies, products or anything. See live at [blog.sendtoinc.com](http://blog.sendtoinc.com)",
    "main": "_site/index.html",
    "dependencies": {
        "inuitcss": "^6.0.0-beta.5"
    },
    "devDependencies": {
        "stylelint": "@^7.10.1",
        "stylelint-config-standard": "^16.0.0"
    },
    "scripts": {
        "dev": "bundle exec jekyll serve",
        "build": "bundle exec jekyll build",
        "test": "echo \"Error: no test specified\" && exit 1",
        "start": "npm run dev"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/lhermann/lmphilippines.git"
    },
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/lhermann/lmphilippines/issues"
    },
    "homepage": "https://github.com/lhermann/lmphilippines#readme",
    "browserslist": ["> 1%", "last 2 versions", "IE 10"],
    "stylelint": {
        "extends": "/usr/local/lib/node_modules/stylelint-config-standard",
        "plugins": [
            "/usr/local/lib/node_modules/stylelint-no-unsupported-browser-features"
        ],
        "rules": {
            "plugin/no-unsupported-browser-features": [
                true,
                {
                    "severity": "warning"
                }
            ],
            "indentation": null,
            "max-empty-lines": 2,
            "number-leading-zero": null
        }
    }
}
